{
  "name": "viciclo",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Chrome extension to make cycles at some crypto brokers",
  "homepage_url": "https://github.com/pvgomes/viciclo",
  "author": "pvgomes",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "src/popup.html",
    "default_title": "Viciclo"
  }
}
